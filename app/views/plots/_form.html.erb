<%= simple_form_for(@plot) do |f| %>
  <div class="form-group">
    <%= f.input :name%>
    <%= f.input :cementary_name %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit %>
<% end %>
